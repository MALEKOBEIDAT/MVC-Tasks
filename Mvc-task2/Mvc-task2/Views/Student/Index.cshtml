@foreach (var Student in ViewBag.Student)
{
   
    <table class="table table-bordered" >
        <tr>
            <td>@Student.ID</td>
        </tr>
        <tr>
            <td>@Student.Name</td>
        </tr>

        <tr>
            <td>@Student.Major</td>
        </tr>
        <tr>
            <td>@Student.Faculity</td>
        </tr>
    </table>



}